! special
*.foreground:   #5f5f5f
*.background:   #ffffd7
*.cursorColor:  #5f5f5f

! black
*.color0:       #252525
*.color8:       #454545

! red
*.color1:       #ff4b4b
*.color9:       #cc0000

! green
*.color2:       #d7ff5f
*.color10:      #00af00

! yellow
*.color3:       #ff8700
*.color11:      #ff5d17

! blue
*.color4:       #5fafd7
*.color12:      #1f5bff

! magenta
*.color5:       #d18aff
*.color13:      #b218b2

! cyan
*.color6:       #87ffff
*.color14:      #87d7af

! white
*.color7:       #9e9e9e
*.color15:      #5f5f5f

st.alpha: 240


!!--urxvt-settings--!!
URxvt.scrollBar: false
URxvt.font: xft:FantasqueSansMono Nerd Font Mono:pixelsize=24
URxvt.depth: 32
URxvt.perl-ext-common: font-size

!!--font-settings--!!
Xft.autohint:  true
Xft.antialias: true
Xft.hinting:   true
Xft.hintstyle: hintslight
Xft.dpi:       150
Xft.lcdfilter: lcddefault
